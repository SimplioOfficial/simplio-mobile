{
  "name": "simplio",
  "version": "0.2.0",
  "author": "Simplio Developers",
  "homepage": "https://simplio.io",
  "description": "Multi asset wallet",
  "scripts": {
    "start": "npm-run-all --parallel cors dev",
    "patch": "gulp copy-node-patches",
    "dev": "gulp getversion && ionic serve",
    "dev-ios": "ionic capacitor run ios -l --external",
    "dev-md": "gulp getversion && ionic cap run android -l --external",
    "dev-lab": "gulp getversion && ionic serve --lab",
    "increase-version": "node increase-version.js",
    "build": "./build.sh",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "cors": "node cors.js",
    "build:ng:prod": "gulp getversion && npm run build:prod:windows && npm run patch && npx cap sync android",
    "build:ios:prod": "gulp getversion && npm run build:prod:mac && npm run patch && npx cap sync ios",
    "build:ng": "set NODE_OPTIONS=--max_old_space_size=12000 && gulp getversion && ionic build && npx cap sync android && npx cap open android",
    "build:ios": "gulp getversion && ionic build && npm run patch && npx cap sync ios && npx cap open ios",
    "build:dev": "gulp getversion && ionic build && npx cap sync",
    "build-apk": "ionic build && npm run patch && npx cap sync android && npx cap open android",
    "sign-android": "rm -f platforms/android/app/build/outputs/apk/release/app-release-signed-aligned.apk && jarsigner -verbose -sigalg SHA1withRSA -digestalg SHA1 -keystore ./bitfrost.keystore -signedjar platforms/android/app/build/outputs/apk/release/app-release-signed.apk platforms/android/app/build/outputs/apk/release/app-release-unsigned.apk bitfrost && $ANDROID_HOME/build-tools/28.0.3/zipalign -v 4 platforms/android/app/build/outputs/apk/release/app-release-signed.apk platforms/android/app/build/outputs/apk/release/app-release-signed-aligned.apk",
    "build:prod:windows": "set NODE_OPTIONS=--max_old_space_size=8192 && ionic build --configuration production --release",
    "build:prod:mac": "export NODE_OPTIONS=--max_old_space_size=8192 && ionic build --configuration production --release"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "12.2.6",
